   -
     code : |1+
      // Modifie le titre selon le mode
      selon gnmode
      	cas acInterdit : LIB_Titre = <§$0000§>
      	CAS acAvertissement : LIB_Titre = <§$0001§>
      	CAS acarret : LIB_Titre = <§$0002§>
      FIN
      
      // Affiche le message
      lib_message = gsmessage
      
      // Définit la temporisation du bouton de fermeture
      DélaiAvantFermeture(mafenetre, BTN_Fermer, gnTempsAffichage*100)
